extends ../layout.pug

block content
    .contenedorVolverCrear
        a.link(href="./")
            | Volver Atrás
            img(src='/images/volver.png', alt="Flecha hacia atrás")
    table.table#tabla
        thead
            tr
                th NUMERO DE LOTE
                th CANTIDAD DE VACUNAS
                th TIPO DE VACUNA
                th EN DEPOSITO PROVINCIAL
                th EN CENTRO DE VACUNACION
        tbody
            each lista in listaVacunasVencidas
                tr
                    td= lista.nroLote
                    td= lista.vencidas
                    td= lista.tipoVacuna
                    td= lista.enProvincia
                    td= lista.enCentro