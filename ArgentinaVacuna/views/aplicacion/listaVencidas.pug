extends ../layout.pug

block content
    .contenedorVolverCrear
        a.link(href="./")
            | Volver Atrás
            img(src='/images/volver.png', alt="Flecha hacia atrás")
    table.table#tabla
        thead
            tr
                th DNI
                th NOMBRE
                th CENTRO DE VACUNACION
                th PROVINCIA
                th TIPO DE VACUNA
        tbody
            each lista in listadoVencidas
                tr
                    td= lista.pacienteDNI
                    td= lista.pacienteNombre
                    td= lista.centro
                    td= lista.provincia
                    td= lista.tipoVacuna

