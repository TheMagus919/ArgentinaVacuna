extends ../layout.pug

block content
    .contenedorVolverCrear
        a.link(href="./")
            | Volver Atrás
            img(src='/images/volver.png', alt="Flecha hacia atrás")
    h4 Listado de Personas que se les aplico Vacunas vencidas:
    table.table#tabla
        thead
            tr
                th DNI
                th NOMBRE
                th CENTRO DE VACUNACION
                th PROVINCIA
                th TIPO DE VACUNA
        tbody
            each lista in listadoVencidas
                tr
                    td= lista.dni
                    td= lista.paciente
                    td= lista.centro
                    td= lista.provincia
                    td= lista.tipoVacuna

